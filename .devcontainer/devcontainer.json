{
  "name": "token-validation-service",
  "dockerComposeFile": "../../orchestration/claude-code-sandbox/docker-compose.yml",
  "service": "claude-dev",
  "workspaceFolder": "/workspace/token-validation-service",
  "shutdownAction": "none",

  "initializeCommand": "bash ${localWorkspaceFolder}/../orchestration/claude-code-sandbox/setup-env.sh",

  "remoteEnv": {
    "SSH_AUTH_SOCK": ""
  },

  "customizations": {
    "vscode": {
      "extensions": [
        "anthropic.claude-code"
      ]
    }
  }
}
